/**index.wxss**/
page{
  background: #FFEFD5;
}
/* 关卡区域列表 */
.levelBox{
  width: 100%;
}

/* 单个关卡区域 */
.box{
  width: 50%;
  float: left;
  margin: 25rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}

/* 选关图片 */
image{
  width: 260rpx;
  height: 260rpx;
  border-radius: 100px;
}